Include config.local

Host *
  # Use my normal ssh keys
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  # Send a keepalive packet every minute
  ServerAliveInterval 60

  # Multiplex connections, keep them alive
  ControlMaster auto
  ControlPath /tmp/%r@%h:%p
  ControlPersist yes
  ForwardAgent yes

