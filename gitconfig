[alias]
la = "!git config -l | grep alias | cut -c 7-"
st = status -s
cl = clone
ci = commit
co = checkout
br = branch 
r = reset
r1 = reset HEAD^
sl = stash list
sa = stash apply
ss = stash save
unstage = reset HEAD --
last = log -1 HEAD
