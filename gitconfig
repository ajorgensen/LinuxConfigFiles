[user]
	name = Chris Jones
	email = chris@christopherjones.us
[alias]
	st = status
	ls = log --graph --pretty=format:'%C(yellow)%h%Creset%C(blue)%d%Creset %C(white bold)%s%Creset %C(white dim)(by %an %ar)%Creset' --all
  fl = log -u
  last = "!git ls -1"
  la = "!git config -l | grep alias | cut -c 7-"
  co = checkout
  ci = commit -v
  st = status -b
  a = add
[push]
	default = current
[color]
	diff = true
